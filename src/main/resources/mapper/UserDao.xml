<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lisihao.demo.dao.UserDao">
    <select id="getUserList" resultType="com.lisihao.demo.entity.User">
        SELECT id, user_name, age, city, create_time, last_edit_time
        FROM user
    </select>
</mapper>